{
  "name": "oa-vue",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode dev",
    "build:dev": "vite build --mode dev",
    "build:test": "vite build --mode test",
    "build:prod": "vite build --mode prod",
    "dev:project-a": "cross-env project=project-a vite --mode dev",
    "build:dev:project-a": "cross-env project=project-a vite build --mode dev",
    "build:test:project-a": "cross-env project=project-a vite build --mode test",
    "build:prod:project-a": "cross-env project=project-a vite build --mode prod",
    "dev:project-b": "cross-env project=project-b vite --mode dev",
    "build:dev:project-b": "cross-env project=project-b vite build --mode dev",
    "build:test:project-b": "cross-env project=project-b vite build --mode test",
    "build:prod:project-b": "cross-env project=project-b vite build --mode prod",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "pinia": "^2.0.32",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "prettier": "^2.8.4",
    "vite": "^4.1.4"
  }
}
